{
	"default": {
		"command": "sudo /do/led/onoff",
		"usage" : "onoff gpio state",
		"args": {
			"gpio": {
				"type": "Config"
			},
			"state": {
				"type": "Post",
				"modifier" : "Binary"
			}
		}
	},
	"blink": {
		"command": "sudo /do/led/blink",
		"usage" : "blink gpio times speed",
		"args": {
				"gpio": {
					"type": "Config"
				},
				"times": {
					"type": "Actions"
				},
				"speed": {
					"type": "Actions"
				}
		}
	}
}