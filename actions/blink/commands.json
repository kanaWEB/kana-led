{
	"default": {
		"command": "sudo /opt/piget/led/bin/onoff",
		"usage" : "onoff gpio state",
		"args": {
			"gpio": {
				"type": "Config"
			},
			"state": {
				"type": "Post",
				"modifier" : "Binary"
			}
		}
	},
	"blink": {
		"command": "sudo /opt/piget/led/bin/blink",
		"usage" : "blink gpio speed times",
		"args": {
				"gpio": {
					"type": "Config"
				},
				"speed": {
					"type": "Actions"
				},
				"times": {
					"type": "Actions"
				}
		}
	},
	"pwm": {
		"command": "sudo /opt/piget/led/bin/pwm",
		"usage" : "pwm gpio speed times",
		"args": {
			"gpio": {
			"type": "Config"
			},
			"speed": {
			"type": "Actions"
			},
			"times": {
			"type": "Actions"
			}
		}
	}
}